<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.JoinActivity" >

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="366dp"
        android:layout_height="2dp"
        android:src="@android:drawable/screen_background_dark_transparent"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="640dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="366dp"
        android:layout_height="2dp"
        android:src="@android:drawable/screen_background_dark_transparent"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="426dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="2dp" />

    <ImageView
        android:id="@+id/image_email"
        android:layout_width="86dp"
        android:layout_height="66dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="179dp" />

    <ImageView
        android:id="@+id/image_password"
        android:layout_width="82dp"
        android:layout_height="72dp"
        app:srcCompat="@drawable/password"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="245dp" />

    <ImageView
        android:id="@+id/image_user"
        android:layout_width="82dp"
        android:layout_height="104dp"
        app:srcCompat="@drawable/user2"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="300dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="255dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="190dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Username"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="332dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="155dp"
        android:layout_height="67dp"
        android:text="Sign up"
        android:textSize="28sp"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="87dp" />

    <Button
        android:id="@+id/btnOK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="650dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="86dp"
        android:layout_height="29dp"
        android:text="Location"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="438dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Interest"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="524dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="107dp"
        android:layout_height="37dp"
        android:text="Button"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="467dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="107dp"
        android:layout_height="37dp"
        android:text="Button"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="553dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="187dp"
        android:layout_height="30dp"
        android:text="GPS 위치 정보 사용"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="598dp" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Off / On"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="601dp" />

</androidx.constraintlayout.widget.ConstraintLayout>